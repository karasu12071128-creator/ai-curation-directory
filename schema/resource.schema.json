{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Resource",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "source_type": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "short_desc": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "author": {
      "type": "string"
    },
    "stars/downloads": {
      "type": [
        "integer",
        "number"
      ]
    },
    "license": {
      "type": "string"
    },
    "added_at": {
      "type": "string"
    },
    "updated_at": {
      "type": "string"
    },
    "lang": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "score": {
      "type": [
        "integer",
        "number"
      ]
    },
    "image_url": {
      "type": [
        "string",
        "null"
      ]
    }
  },
  "required": [
    "id",
    "source_type",
    "title",
    "url"
  ]
}
